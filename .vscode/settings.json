{
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "makefile.configureOnOpen": false,
    "cSpell.words": [
        "Clawd",
        "deinit",
        "deiniting",
        "Doptimize",
        "Dtarget",
        "getenv",
        "groq",
        "opencode",
        "openrouter",
        "orelse",
        "Pico",
        "satibot",
        "Sigaction",
        "sigset",
        "zigdoc",
        "ziglint"
    ]
}
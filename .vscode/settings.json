{
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "makefile.configureOnOpen": false,
    "cSpell.words": [
        "deinit",
        "Doptimize",
        "Dtarget",
        "getenv",
        "groq",
        "openrouter",
        "orelse",
        "satibot",
        "Sigaction",
        "sigset"
    ]
}